		"textBlockQuote.background": "#000000",
		"textBlockQuote.border": "#666666",
		"textCodeBlock.background": "#000000",
		"textLink.activeForeground": "#ff0077",
		"textLink.foreground": "#ff0077",
		"textPreformat.foreground": "#dddddd",
		"textSeparator.foreground": "#dddddd",
		"button.background": "#222222",
		"button.foreground": "#dddddd",
		"button.hoverBackground": "#000000",
