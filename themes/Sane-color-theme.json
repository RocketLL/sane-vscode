	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#666666",
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"punctuation.separator"
			],
			"settings": {
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "Other Keywords",
			"scope": [
				"keyword.other"
			],
			"settings": {
				"foreground": "#aaaaaa",
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"string"
			],
			"settings": {
				"foreground": "#ff0077"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#dddddd",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Key",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#dddddd",
			}
		}
	]
}